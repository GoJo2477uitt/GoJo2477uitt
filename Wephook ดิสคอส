local url = Unt
if not url then
    warn("Webhook (Unt) not found")
    return
end

local Players = game:GetService("Players")
local MarketplaceService = game:GetService("MarketplaceService")
local HttpService = game:GetService("HttpService")

local gamename = MarketplaceService:GetProductInfo(game.PlaceId).Name
local gameid = game.PlaceId

local req = (syn and syn.request) or http_request or request

Players.PlayerAdded:Connect(function(player)
    task.wait(1)

    local countPlr = #Players:GetPlayers()

    local data = {
        username = "Quantum hub",
        avatar_url = "https://cdn.discordapp.com/emojis/1449778405523591270.webp?size=128",
        embeds = {
            {
                title = "âœ… Player Joined",
                description = "**Players : ** "..countPlr,
                color = 121793,

                fields = {
                    { name = "Game Name", value = "```"..gamename.."```" },
                    { name = "Game ID", value = "```"..gameid.."```", inline = true },
                    { name = "Job ID", value = "```"..game.JobId.."```", inline = true },
                    { name = "Player", value = "```"..player.Name.."```", inline = true },
                    { name = "UserId", value = "```"..player.UserId.."```", inline = true },
                    {
                        name = "Profile",
                        value = "```https://www.roblox.com/users/"..player.UserId.."/profile```"
                    }
                },

                image = {
                    url = "https://cdn.discordapp.com/attachments/1449624300280610849/1450103210525327481/pain-naruto.gif"
                },

                thumbnail = {
                    url = "https://cdn.discordapp.com/emojis/1449778405523591270.webp?size=128"
                },

                footer = {
                    text = "Powered by Quantum hub",
                    icon_url = "https://cdn.discordapp.com/emojis/1449778405523591270.webp?size=64"
                },

                timestamp = DateTime.now():ToIsoDate()
            }
        }
    }

    req({
        Url = url,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(data)
    })
end)
